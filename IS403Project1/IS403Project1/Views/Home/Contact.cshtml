@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<address>
    One Group 8 Rocks Road<br />
    Provo, Ut 84604<br />
    <abbr title="Phone">P:</abbr>
    4801.422.8888
</address>

<address>
    <strong>Support:</strong>   <a href="Tannermorse.net">Tannermorse.net</a><br />
</address>

<div class="container">
    <form>
        <div class="form-group">
            <label for="email">Email:</label>
            <input type="email" class="form-control" id="email" placeholder="Enter email" name="email">
        </div>
        <div class="form-group">
            @Html.DropDownList("subjects")
        </div>
        <div class="form-group">
            <label>Body:</label>
            <textarea class="form-control" id="body" placeholder="Body" name="body" style="height: 150px;"></textarea>
        </div>
        <button type="submit" class="btn btn-default">Submit</button>
    </form>
</div>
