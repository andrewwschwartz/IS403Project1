@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<address>
    One Group 8 Rocks Road<br />
    Provo, Ut 84604<br />
    <abbr title="Phone">P:</abbr>
    4801.422.8888
</address>

<address>
    <strong>Support:</strong>   <a href="Tannermorse.net">Tannermorse.net</a><br />
</address>

<div class="container">
    <form>
        <div class="form-group">
            <label for="email">Email:</label>
            <input type="email" class="form-control" id="email" placeholder="Enter email" name="email">
        </div>
        <div class="form-group">
            <label>Subject:</label>
            <ul class="nav nav-pills left">
                <li class="dropdown active span8">
                    <a class="dropdown-toggle" id="inp_impact" data-toggle="dropdown">
                        <i class="icon icon-envelope icon-white"></i>
                        <span id="text">@ViewBag.Subject</span><span class="caret"></span>
                    </a>
                    <ul ID="divNewNotifications" class="dropdown-menu">
                        <li onclick="updateSubject"><a>One</a></li>
                        <li><a>Two</a></li>
                        <li><a>Three</a></li>
                    </ul>
                </li>
            </ul>
            <br />
            <input class="form-control" id="Subject" placeholder="Select Subject" name="Subject">
        </div>
        <div class="form-group">
            <label>Body:</label>
            <textarea class="form-control" id="body" placeholder="Body" name="body" style="height: 150px;"></textarea>
        </div>
        <button type="submit" class="btn btn-default">Submit</button>
    </form>
</div>
